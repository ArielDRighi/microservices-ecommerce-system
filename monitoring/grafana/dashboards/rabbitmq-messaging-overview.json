{
  "dashboard": {
    "id": null,
    "uid": "rabbitmq-messaging",
    "title": "RabbitMQ - Messaging Overview",
    "tags": ["rabbitmq", "messaging", "events", "microservices"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "type": "graph",
        "title": "Event Publish Rate (Go Publisher)",
        "targets": [
          {
            "expr": "rate(inventory_events_published_total{status=\"success\"}[5m])",
            "legendFormat": "{{event_type}} - {{routing_key}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Events/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 2,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "type": "graph",
        "title": "Event Consumption Rate (NestJS Consumer)",
        "targets": [
          {
            "expr": "rate(orders_events_consumed_total{status=\"success\"}[5m])",
            "legendFormat": "{{event_type}} - {{routing_key}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Events/sec"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 3,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "type": "graph",
        "title": "Publish Duration (P95, P99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(inventory_events_publish_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 - {{event_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(inventory_events_publish_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 - {{event_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 4,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "type": "graph",
        "title": "Processing Duration (P95, P99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(orders_events_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 - {{event_type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.99, rate(orders_events_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "P99 - {{event_type}}",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "s",
            "label": "Duration"
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 5,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 16
        },
        "type": "stat",
        "title": "Dead Letter Queue Messages",
        "targets": [
          {
            "expr": "sum(orders_events_dlq_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "textMode": "auto"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "yellow",
              "value": 10
            },
            {
              "color": "red",
              "value": 50
            }
          ]
        }
      },
      {
        "id": 6,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 16
        },
        "type": "stat",
        "title": "Idempotent Skips",
        "targets": [
          {
            "expr": "sum(orders_events_idempotent_skips_total)",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "textMode": "auto"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "color": "blue",
              "value": null
            }
          ]
        }
      },
      {
        "id": 7,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 16
        },
        "type": "stat",
        "title": "Publish Errors",
        "targets": [
          {
            "expr": "sum(rate(inventory_events_publish_errors_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "textMode": "auto"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "yellow",
              "value": 0.01
            },
            {
              "color": "red",
              "value": 0.05
            }
          ]
        },
        "unit": "reqps"
      },
      {
        "id": 8,
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 16
        },
        "type": "stat",
        "title": "Processing Errors",
        "targets": [
          {
            "expr": "sum(rate(orders_events_processing_errors_total[5m]))",
            "refId": "A"
          }
        ],
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "orientation": "auto",
          "reduceOptions": {
            "calcs": ["lastNotNull"]
          },
          "textMode": "auto"
        },
        "thresholds": {
          "mode": "absolute",
          "steps": [
            {
              "color": "green",
              "value": null
            },
            {
              "color": "yellow",
              "value": 0.01
            },
            {
              "color": "red",
              "value": 0.05
            }
          ]
        },
        "unit": "reqps"
      },
      {
        "id": 9,
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 24
        },
        "type": "graph",
        "title": "RabbitMQ Queue Length (orders.inventory_events)",
        "targets": [
          {
            "expr": "rabbitmq_queue_messages{queue=\"orders.inventory_events\"}",
            "legendFormat": "Ready Messages",
            "refId": "A"
          },
          {
            "expr": "rabbitmq_queue_messages_unacked{queue=\"orders.inventory_events\"}",
            "legendFormat": "Unacked Messages",
            "refId": "B"
          }
        ],
        "yaxes": [
          {
            "format": "short",
            "label": "Messages"
          },
          {
            "format": "short"
          }
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {
                "params": [1000],
                "type": "gt"
              },
              "operator": {
                "type": "and"
              },
              "query": {
                "params": ["A", "5m", "now"]
              },
              "reducer": {
                "params": [],
                "type": "avg"
              },
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "name": "Queue Length Alert",
          "noDataState": "no_data"
        }
      },
      {
        "id": 10,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 32
        },
        "type": "graph",
        "title": "Handler Execution Success Rate",
        "targets": [
          {
            "expr": "rate(orders_events_handler_executions_total{status=\"success\"}[5m]) / (rate(orders_events_handler_executions_total{status=\"success\"}[5m]) + rate(orders_events_handler_executions_total{status=\"failed\"}[5m]))",
            "legendFormat": "{{handler_name}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "percentunit",
            "label": "Success Rate",
            "max": 1,
            "min": 0
          },
          {
            "format": "short"
          }
        ]
      },
      {
        "id": 11,
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 32
        },
        "type": "graph",
        "title": "Publish Retry Rate",
        "targets": [
          {
            "expr": "rate(inventory_events_publish_retries_total[5m])",
            "legendFormat": "{{event_type}} - Attempt {{attempt}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          {
            "format": "reqps",
            "label": "Retries/sec"
          },
          {
            "format": "short"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"]
    }
  },
  "overwrite": true
}
